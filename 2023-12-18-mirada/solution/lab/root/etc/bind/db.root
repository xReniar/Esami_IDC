$TTL 60000
@ IN SOA ROOT-SERVER. root.ROOT-SERVER. (
    2023121401 ; serial
    28800 ;  refresh
    14400 ; retry
    3600000 ; expire
    0 ; negative cache ttl
)

@                  IN   NS   ROOT-SERVER.
ROOT-SERVER.       IN    A   3.1.0.2

net.           IN   NS   dns.net.
dns.net.       IN    A   2.1.0.2